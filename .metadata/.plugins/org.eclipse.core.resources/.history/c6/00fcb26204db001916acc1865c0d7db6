package com.gregory.hospitalapp.dao.interfaces;

import java.util.List;

import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.CrudRepository;

import com.gregory.hospitalapp.model.*;

public interface PacientesDao  extends CrudRepository<Paciente, Long> {
	List<Paciente> findAllByGlic();
	
	@Query("SELECT * FROM pacientes where glicose>100")
	Paciente findByGlic100();
}
