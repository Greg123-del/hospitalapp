package com.gregory.hospitalapp.dao.interfaces;

import java.util.List;

import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.CrudRepository;

import com.gregory.hospitalapp.model.*;


public interface MedicoDao extends CrudRepository<Medico, Long> {
	List<Medico> findAllByOrderByIdAsc();
	
	@Query("SELECT * FROM medicos m left join pacientes p on m.ID_MEDICO = p.ID_MEDICO_FK where ID_MEDICO_FK is null")
	Medico find
}
