package com.gregory.hospitalapp.dao.interfaces;

import java.util.List;

import org.springframework.data.jpa.repository.Query;
import org.springframework.data.repository.CrudRepository;

import com.gregory.hospitalapp.model.*;


public interface MedicoDao extends CrudRepository<Medico, Long> {
	List<Medico> nome();
	
	@Query("select m from Medico m where m.nome = ?1")
	Medico findByUsername(String nome);
	
	@Query("select m from Medico m where m.nome = ?1 and m.ID_MEDICO != ?2")
	Medico findByUsername(String nome, Long ID_MEDICO);
}
